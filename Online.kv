<OnlineScreen>:
    name: "Online"
    TextInput:
        id: input
        multiline: False
        input_filter: 'int'
        font_size: root.height*0.145
        font_name: 'Arial'
        size_hint: .7, .2
        pos_hint: {'x': .05,'center_y': .85}
        background_color: (1, 1, 1, 1)
        foreground_color: (0,0,0,1)
        on_text: root.chkUserNum()
        on_text_validate:
            root.value = int(self.text)
            root.validate(0, 99999999)
    FloatLayout:
        Label:
            pos_hint: {'x': .825, 'y': 0.175}
            canvas:
                Color:
                    rgba: rgba("#4fd188")
                RoundedRectangle:
                    size: root.width*0.15,root.height*0.12
                    pos: self.pos
                    radius: root.width*0.03,root.width*0.03,root.width*0.03,root.width*0.03
        Button:
            id: submit
            text: "Шалгах"
            font_size: root.height*.04
            font_size: root.width*.03
            pos_hint: {'x': .835, 'y': 0.175}
            size_hint: .13, .13
            background_normal: ''
            background_color: 1,1,1,0
            border: 30, 30, 30, 30
            on_press:
                root.register()


    FloatLayout:
        Label:
            pos_hint: {'x': .825, 'y': 0.025}
            canvas:
                Color:
                    rgba: rgba("#d64f66")
                RoundedRectangle:
                    size: root.width*0.15,root.height*0.12
                    pos: self.pos
                    radius: root.width*0.03,root.width*0.03,root.width*0.03,root.width*0.03
        Button:
