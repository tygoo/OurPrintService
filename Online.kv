<Buttons@Button>:
    font_size: root.height*0.6
    background_color: 1,1,1,0
    background_normal: ''
    color: 0,0,0,1
    size_hint: 0.0725, 0.1
    button_x: 0.1
    button_y: .2
<Button_delete@Button>:
    font_size: root.height*0.4
    background_color: 1,1,1,0
    background_normal: ''
    color: 1,1,1,1
    size_hint: 0.152, 0.1
    button_x: 0.1
    button_y: .2
<Label_but@Label>:
    size_hint: 0.0725, 0.1
    button_x: 0.1
    button_y: .2
    canvas:
        Color:
            rgba: rgba("#d9d9d9")
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: root.width*0.15,root.width*0.15,root.width*0.15,root.width*0.15
<Label_del_but@Label>:
    size_hint: 0.152, 0.1
    button_x: 0.1
    button_y: .2
    canvas:
        Color:
            rgba: rgba("#e36666")
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: root.width*0.075,root.width*0.075,root.width*0.075,root.width*0.075
<OnlineScreen>:
    name: "Online"
    TextInput:
        id: input
        multiline: False
        input_filter: 'int'
        font_size: root.height*0.145
        font_name: 'Arial'
        size_hint: .7, .2
        pos_hint: {'x': .135,'center_y': .85}
        background_color: (1, 1, 1, 1)
        foreground_color: (0,0,0,1)
        on_text: root.chkUserNum()
        on_text_validate:
            root.value = int(self.text)
            root.validate(0, 99999999)
    FloatLayout:
        Label:
            pos_hint: {'x': 0.005, 'y': -0.0055}
            canvas:
                Rectangle:
                    source: 'shadow1.png'
                    size: root.width*0.205,root.height*0.185
                    pos: self.pos
        Label:
            pos_hint: {'x': .035, 'y': 0.03}
            canvas:
                Color:
                    rgba: rgba("#59b9e3")
                RoundedRectangle:
                    size: root.width*0.15,root.height*0.12
                    pos: self.pos
                    radius: root.width*0.03,root.width*0.03,root.width*0.03,root.width*0.03
        Button:
            id: submit
            text: "Шалгах"
            font_size: root.height*.04
            font_size: root.width*.03
            pos_hint: {'x': .045, 'y': 0.029}
            size_hint: .13, .13
            background_normal: ''
            background_color: 1,1,1,0
            border: 30, 30, 30, 30
            on_press:
                root.register()


    FloatLayout:
